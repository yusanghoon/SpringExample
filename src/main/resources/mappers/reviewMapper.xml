<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.agora12.spring.ex.mybatis.dao.ReviewDAO">
  
  	<select id="selectReview" resultType="com.agora12.spring.ex.mybatis.model.Review"
  	parameterType="int">
  		SELECT
  			`id`
  			,`storeId`
  			,`menu`
  			,`userName`
  			,`point`
  			,`review`
  			,`createdAt`
  			,`updatedAt`
  		FROM
  			`new_review`
  		WHERE
  			`id` = #{id}
  	
  	</select>
  
  </mapper>
  
